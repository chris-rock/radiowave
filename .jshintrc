{
    "asi": true,
    "bitwise" : true, 
    "camelcase": true,
    "eqeqeq": true,
    "eqnull": true,
    "globalstrict": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxparams": 4,
    "maxdepth": 3,
    "maxstatements": 20,
    "maxcomplexity": 8,
    "maxlen": 120,
    "multistr"  : true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "sub" : true,
    "trailing": true,
    "node": true,
    "predef": [ "-Promise" ] // https://github.com/jshint/jshint/issues/1747
}